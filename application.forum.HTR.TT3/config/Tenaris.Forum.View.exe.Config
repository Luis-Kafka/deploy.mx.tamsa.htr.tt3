<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="ForumControllerFactoryConfiguration.Proxy" type="Tenaris.Library.Proxy.Factories.GenericProxyFactoryConfigurationSection, Tenaris.Library.Proxy"/>
    <section name="ForumViewConfiguration" type="Tenaris.Forum.View.Shared.Configuration.ForumViewConfigurationSection, Tenaris.Forum.View.Shared"/>
    <section name="ForumControllerFactoryConfiguration" type="Tenaris.Manager.Forum.Common.Configuration.ForumControllerConfigurationSection, Tenaris.Manager.Forum.Common"/>
    <section name="log" type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log"/>
  </configSections>

  <ForumViewConfiguration
		advanceUser="true"
		startInTaskBar="false"
		isReadOnly="false"
		activateOnStartUp="false"
		activateLogOnStartUp="true">
    <ForumControllers>
      <Controller name="TT3" configurationName="ForumControllerFactoryConfiguration.Proxy"/>
    </ForumControllers>
  </ForumViewConfiguration>

  <ForumControllerFactoryConfiguration.Proxy
		url="tcp://127.0.0.1:49101/ForumManager/Controller.soap"
		instanceType="Tenaris.Manager.Forum.Common.IForumController, Tenaris.Manager.Forum.Common"
		aliveInterval="0" forumPresent="false" isTargetManager="false" validateConnection="false"/> 
  
  <ForumControllerFactoryConfiguration
		isSingleton="false"
		address="ForumManager/Controller.soap"
  />

  <system.runtime.remoting>
    <application name="Forum">
      <channels>
        <channel ref="tcp" port="0">
          <clientProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full"/>
          </serverProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>

  <log	
	maxAge="1.00:00:00.000" splitLogPerDay="false" append="false" msgFormat="{TypeChar} {Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{ThreadId,6}] [lap={LapFriendlyValue,-7:f2}{LapFriendlyUnit,3}] {AssemblyName,30:s-30} {IdentString} {Message}" dumpStack="true" maxLogDepth="0" traceAttributeLogLevel="Debug" noQueue="false" maxQueueSize="10000" includeAssembliesRegEx="" excludeAssembliesRegEx="" includeFullClasNameRegEx="" excludeFullClasNameRegEx="" excludePropertyGetterAndSetter="true" flushInterval="1000" logAssemblyLoad="true" colorConsoleLog="true" maxFileSize="4096" maxFileCount="100" traceAttributeLogEnabled="true" additionalLogDestination="None" logLevel="Debug"
	logBaseName="ForumView" path="C:\Proyectos\TT3\DeployTemplate\Nuevacarpeta\HTR_MX\v2.0\TT3\Data\Log\ForumView\" 
  />

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Binaries;"/>
    </assemblyBinding>
  </runtime>
	
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
</configuration>
