<?xml version="1.0" encoding="utf-8" ?>
<configuration>  
  <configSections>
    <section name="ConfigurationProvider" type="Tenaris.Library.Framework.Config.ConfigurationProviderSection, Tenaris.Library.Framework"/>
    <section name="log" type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log"/>
  </configSections>

  <ConfigurationProvider>
    <Providers>
      <AppConfigProvider filename="..\ConnectionStrings.config"/>
      <DbConfigProvider dbSessionName="dbLevel2" areaCode="TT3" zoneCode="" machineCode="" applicationCode="" />
      <LocalConfigProvider />
    </Providers>
  </ConfigurationProvider>

 
  <system.runtime.remoting>
    <application name="Server">
      <channels>
        <channel ref="tcp" port="%port%" timeout="864000">
          <clientProviders>
            <provider type="Tenaris.Library.Proxy.Remoting.ClientRemotingInterceptorProvider, Tenaris.Library.Proxy"/>
            <provider type="Tenaris.Library.Transactions.Sink.ClientTransactionSinkProvider, Tenaris.Library.Transactions.Sink">
              <filter urlRegexToExclude=".*Tenaris.Service.OCR.Remoting.ManagerRemoting.soap.*" />
            </provider>
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
        <!--<provider type="Tenaris.Library.Framework.Remoting.Sinks.ServerLogContextSinkProvider, Tenaris.Library.Framework"/>-->
            <provider type="Tenaris.Library.Transactions.Sink.ServerTransactionSinkProvider, Tenaris.Library.Transactions.Sink"/>
            <provider type="Tenaris.Service.Security.Sink.ServerSecuritySinkProvider, Tenaris.Service.Security.Sink"/>      
          </serverProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off" />
  </system.runtime.remoting>
  
  <log
	maxAge="1.00:00:00.000" splitLogPerDay="false" append="false" msgFormat="{TypeChar} {Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{ThreadId,6}] [lap={LapFriendlyValue,-7:f2}{LapFriendlyUnit,3}] {AssemblyName,30:s-30} {IdentString} {Message}" dumpStack="true" maxLogDepth="0" traceAttributeLogLevel="Debug" noQueue="false" maxQueueSize="10000" includeAssembliesRegEx="" excludeAssembliesRegEx="" includeFullClasNameRegEx="" excludeFullClasNameRegEx="" excludePropertyGetterAndSetter="true" flushInterval="1000" logAssemblyLoad="true" colorConsoleLog="true" maxFileSize="4096" maxFileCount="100" traceAttributeLogEnabled="true" additionalLogDestination="None" logLevel="Debug" 
	logBaseName="%ManagerName%" path="C:\Proyectos\TT3\DeployTemplate\Nuevacarpeta\HTR_MX\v2.0\TT3\Data\Log\Managers\%ManagerName%\" 
  />

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Binaries;"/>
    </assemblyBinding>
  </runtime>
  
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>
