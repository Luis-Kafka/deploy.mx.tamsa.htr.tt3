<UserControl x:Class="System.Windows.Controls.UserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dialogs="clr-namespace:Tenaris.View.Tracking.Dialogs;assembly=Tenaris.View.Tracking.Dialogs"
             >

    <Grid>
        <dialogs:DialogCrane ZoneInfo="{Binding ZoneInfo}" CraneInfo="{Binding CraneZoneInfo}"
                             ZoneConfig="{Binding ZoneConfig}" CraneZoneConfig="{Binding CraneZoneConfig}" 
                             />
    </Grid>
</UserControl>
