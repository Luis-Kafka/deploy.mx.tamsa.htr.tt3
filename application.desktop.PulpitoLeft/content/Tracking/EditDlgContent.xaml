<UserControl x:Class="System.Windows.Controls.UserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ui="clr-namespace:Tenaris.Library.UI.Framework;assembly=Tenaris.Library.UI.Framework"
             xmlns:dialogs="clr-namespace:Tenaris.View.Tracking.Dialogs;assembly=Tenaris.View.Tracking.Dialogs"
			 >

	<Grid>
        <dialogs:DialogEdit TrackingList="{Binding TrackingList}" ZoneInfo="{Binding ZoneInfo}"  ZoneConfig="{Binding ZoneConfig}" AllowChangeHeat="false" 
		CustomField1="OrderNumber" CustomField2="HeatNumber"  Width="1000" />
    </Grid>
	
</UserControl>
