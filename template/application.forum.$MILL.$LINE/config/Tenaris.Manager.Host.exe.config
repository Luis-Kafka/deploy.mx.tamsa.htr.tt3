<?xml version="1.0" encoding="utf-8" ?>
<configuration>  
  <configSections>
    <section name="ConfigurationProvider" type="Tenaris.Library.Framework.Config.ConfigurationProviderSection, Tenaris.Library.Framework"/>
    <section name="log" type="Tenaris.Library.Log.LogConfiguration, Tenaris.Library.Log"/>
  </configSections>

  <ConfigurationProvider>
    <Providers>
      <AppConfigProvider filename="..\ConnectionStrings.config"/>
      <DbConfigProvider dbSessionName="dbLevel2" areaCode="$AREA_CODE" zoneCode="" machineCode="" applicationCode="" />
      <LocalConfigProvider />
    </Providers>
  </ConfigurationProvider>

 
  <system.runtime.remoting>
    <application name="Server">
      <channels>
        <channel ref="tcp" port="%port%" timeout="864000">
          <clientProviders>
            <provider type="Tenaris.Library.Proxy.Remoting.ClientRemotingInterceptorProvider, Tenaris.Library.Proxy"/>
            <provider type="Tenaris.Library.Transactions.Sink.ClientTransactionSinkProvider, Tenaris.Library.Transactions.Sink">
              <filter urlRegexToExclude=".*Tenaris.Service.OCR.Remoting.ManagerRemoting.soap.*" />
            </provider>
            <formatter ref="binary" typeFilterLevel="Full" />
          </clientProviders>
          <serverProviders>
            <formatter ref="binary" typeFilterLevel="Full" />
        <!--<provider type="Tenaris.Library.Framework.Remoting.Sinks.ServerLogContextSinkProvider, Tenaris.Library.Framework"/>-->
            <provider type="Tenaris.Library.Transactions.Sink.ServerTransactionSinkProvider, Tenaris.Library.Transactions.Sink"/>
            <provider type="Tenaris.Service.Security.Sink.ServerSecuritySinkProvider, Tenaris.Service.Security.Sink"/>      
          </serverProviders>
        </channel>
      </channels>
    </application>
    <customErrors mode="Off" />
  </system.runtime.remoting>
  
  <log
	$LOG 
	logBaseName="%ManagerName%" path="$INSTALLATION_PATH\Data\Log\Managers\%ManagerName%\" 
  />

  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <probing privatePath="Binaries;"/>
    </assemblyBinding>
  </runtime>
  
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>

</configuration>
